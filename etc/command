systemctl enable fstrim.timer
systemctl disable rtkit-daemon.service
systemctl mask getty@tty{3,4,5,6}.service
systemctl disable systemd-userdbd.socket
systemctl disable systemd-userdbd.service
systemctl disable polkit.service
systemctl disable systemd-networkd-wait-online.service


###########################################

#command
systemctl disable systemd-resolved

###########################################

systemctl enable iwd.service

systemctl disable systemd-networkd.service
systemctl disable systemd-networkd.socket
systemctl disable systemd-network-generator.service
systemctl enable systemd-resolved.service

ln -sf /run/systemd/resolve/stub-resolv.conf /etc/resolv.conf

#iwd
[General]
EnableNetworkConfiguration=true
route_priority_offset=300

[Scan]
DisablePeriodicScan=true

[Network]
NameResolvingService=systemd
#iwd
